{
  "manifest_version": 2,

  "name": "GFW Timeout",
  "description": "This is a extension to handle GFW timeouts to speed up page loading",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.gstatic.com/"
  ],
  "background": {
    "scripts": ["popup.js"],
    "persistent": true
  }
}
