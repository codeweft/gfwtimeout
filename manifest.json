{
  "manifest_version": 2,

  "name": "GFW Timeout",
  "description": "This is a extension to handle GFW timeouts to speed up page loading",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "opts.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*/"
  ],
  "background": {
    "scripts": ["js/angular.js","background.js"],
    "persistent": true,
    "default_icon": "icon.png",
    "default_popup": "opts.html"
  },

  "options_page": "opts.html"
}
